  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shechoruiz/webpack/issues"
  },
  "homepage": "https://github.com/shechoruiz/webpack#readme",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.1.0",
    "css-loader": "2.1.1",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "3.2.0",
    "less": "3.12.2",
    "less-loader": "6.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.14.1",
    "sass-loader": "9.0.3",
    "style-loader": "0.23.1",
    "stylus": "0.54.8",
    "stylus-loader": "3.0.2",
    "url-loader": "4.1.0",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
